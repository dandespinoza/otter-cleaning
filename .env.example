# Otter Cleaning - Environment Configuration
# Copy this file to .env and update the values for your environment

# Site Configuration
VITE_SITE_URL=https://ottercleaning.com
VITE_SITE_NAME="Otter Cleaning"

# API Configuration (for future backend integration)
VITE_API_URL=https://api.ottercleaning.com
VITE_API_TIMEOUT=30000

# Contact Information
VITE_CONTACT_EMAIL=hello@ottercleaning.com
VITE_CONTACT_PHONE=(929) 274-1177
VITE_COMMERCIAL_EMAIL=hello@ottercleaning.com

# Social Media
VITE_TWITTER_HANDLE=@OtterCleaning
VITE_FACEBOOK_URL=https://facebook.com/OtterCleaning
VITE_INSTAGRAM_URL=https://instagram.com/OtterCleaning

# Analytics (optional)
# VITE_GA_TRACKING_ID=G-XXXXXXXXXX
# VITE_GTM_ID=GTM-XXXXXXX

# Google Maps API (for address autocomplete)
# Get your API key at: https://console.cloud.google.com/apis/credentials
# Enable: Maps JavaScript API and Places API
# VITE_GOOGLE_MAPS_API_KEY=AIzaSyXXXXXXXXXXXXXXXXXXXXXXXXXXX

# Payment Processing (for future integration)
# VITE_STRIPE_PUBLISHABLE_KEY=pk_live_XXXXXXXX

# Feature Flags
VITE_ENABLE_BOOKING=true
VITE_ENABLE_COMMERCIAL=true
VITE_ENABLE_PROMO_CODES=true

# Development
VITE_DEV_MODE=false
